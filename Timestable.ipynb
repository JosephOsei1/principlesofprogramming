    "# Get user input\n",
    "num = int(input(\"Enter a number: \"))\n",
    "\n",
    "# Generate and display the times table\n",
    "print(f\"\\nTimes Table for {num}:\\n\")\n",
    "for i in range(1, 13):\n",
    "    print(f\"{num} × {i} = {num * i}\")\n"
